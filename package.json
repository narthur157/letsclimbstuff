    "test": "npm run api && tape server/**/*.spec.js | tap-spec",
    "dev": "concurrently --kill-others \"tsc -w -p server/ \" \"nodemon --watch server/dist --delay 2000ms server/dist/server.js\" \"webpack-dev-server --config webpack.dev.js --host 0.0.0.0 \"",
